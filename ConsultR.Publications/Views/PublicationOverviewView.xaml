<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    x:Class="ConsultR.Publications.Views.PublicationOverviewView"
             mc:Ignorable="d" 
    xmlns:ViewModels="clr-namespace:ConsultR.Publications.ViewModels"
             d:DataContext="{d:DesignInstance Type=ViewModels:PublicationOverviewDesignViewModel, IsDesignTimeCreatable=true}"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
        <Label Content="{Binding SelectedPublication.Name}">
        </Label>
    </Grid>
</UserControl>
